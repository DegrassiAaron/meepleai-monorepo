    "Editor": {
      "MaxTokens": 500,
      "RefillRate": 5.0
    },
    "User": {
      "MaxTokens": 100,
      "RefillRate": 1.0
    },
    "Anonymous": {
      "MaxTokens": 60,
      "RefillRate": 1.0
    }
  }
}
