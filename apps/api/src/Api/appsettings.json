{
  "AllowedOrigins": [ "http://localhost:3000" ],
  "ConnectionStrings": {
    "Postgres": "Host=localhost;Database=meepleai;Username=meeple;Password=meeplepass"
  },
  "QdrantUrl": "http://localhost:6333",
