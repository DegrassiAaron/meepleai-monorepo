{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Authentication": {
    "SessionCookie": {
      "Secure": false,
      "SameSite": "Lax",
      "HttpOnly": true
    },
    "SessionManagement": {
      "InactivityTimeoutDays": 7,
      "AutoRevocationIntervalHours": 1
    }
  }
}
